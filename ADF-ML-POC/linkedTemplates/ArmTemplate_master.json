{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-ML-POC"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"RestService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService1'"
		},
		"RestService2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService2'"
		},
		"SQLdblinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLdblinkedservice'"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://40.117.44.126:8000/v1/documents?uri=/binary/sampleDoc"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"RestService2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://20.193.229.230:8000/v1/documents?uri=/binary/sampleTestDoc"
		},
		"RestService2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"azurepoctrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d31b354-b40a-431b-8f85-822d19c3d7da/resourceGroups/ml-poc/providers/Microsoft.Storage/storageAccounts/mlpoc"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d31b354-b40a-431b-8f85-822d19c3d7da/resourceGroups/ml-poc/providers/Microsoft.Storage/storageAccounts/mlpoc"
		},
		"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "ortambo.database.windows.net"
		},
		"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "sanchitBora"
		},
		"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-ML-POC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"RestService1_password": {
						"value": "[parameters('RestService1_password')]"
					},
					"RestService2_password": {
						"value": "[parameters('RestService2_password')]"
					},
					"SQLdblinkedservice_connectionString": {
						"value": "[parameters('SQLdblinkedservice_connectionString')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					},
					"RestService2_properties_typeProperties_url": {
						"value": "[parameters('RestService2_properties_typeProperties_url')]"
					},
					"RestService2_properties_typeProperties_userName": {
						"value": "[parameters('RestService2_properties_typeProperties_userName')]"
					},
					"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('integrationRuntime2_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"azurepoctrigger_properties_typeProperties_scope": {
						"value": "[parameters('azurepoctrigger_properties_typeProperties_scope')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}